$('#login_extras').html('');
$('#login_extras2').html('');

<% @result["componentList"].each do |field| %>
  $('#login_extras').append('<%= label_tag field["displayName"] %>');
  $('#login_extras').append('<%= text_field_tag field["valueIdentifier"] %>');
  $('#login_extras').append('<br />');
<% end %>

<% @result["componentList"].each do |field| %>
  $('#login_extras2').append('<%= label_tag field["displayName"] %>');
  $('#login_extras2').append('<%= text_field_tag field["valueIdentifier"] %>');
  $('#login_extras2').append('<br />');
<% end %>
